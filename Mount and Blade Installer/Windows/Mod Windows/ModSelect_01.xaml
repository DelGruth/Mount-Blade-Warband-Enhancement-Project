        <Button Content="Better Skyboxes" HorizontalAlignment="Left" Margin="36,154,0,0" VerticalAlignment="Top" Width="141" Height="30" Click="Button_Click_5"/>
        <Button Content="Realistic Colors" HorizontalAlignment="Left" Margin="36,189,0,0" VerticalAlignment="Top" Width="141" Height="30" Click="Button_Click_6"/>
        <Button Content="Alternate Warband" HorizontalAlignment="Left" Margin="36,224,0,0" VerticalAlignment="Top" Width="141" Height="30" Click="Button_Click_7"/>
